{
    "database": {
        "client": "sqlite3",
        "connection": {
            "filename": "content/data/ghost.db"
        },
        "debug": false
    },
    "paths": {
        "contentPath": "content/"
    },
    "caching": {
		"frontend": {
			"maxAge": 600
		}
	},
	"storage": {
    "active": "ghost-azure-storage",
        "ghost-azure-storage": {
          "connectionString": AZURE_STORAGE_CONNECTION_STRING,
          "container": "assets"
        }
  },
}
